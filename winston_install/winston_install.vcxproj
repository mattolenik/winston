    <APPVEYOR_BUILD_VERSION Condition="'$(APPVEYOR_BUILD_VERSION)' == ''">0.0.1.0</APPVEYOR_BUILD_VERSION>
    <BuildVer>$(APPVEYOR_BUILD_VERSION)</BuildVer>
  </PropertyGroup>
  <!-- Embedding Winston -->
  <Target Name="EmbedWinston" BeforeTargets="PrepareForBuild">
    <Exec Command="tools\xd.exe -dwinston_tar &quot;$(WinstonTar)&quot; &gt; winston_tar.bin.cpp" />
    <GetFileSize FileName="$(WinstonTar)">
      <Output TaskParameter="FileSize" PropertyName="TarSize" />
    </GetFileSize>
    <ReplaceInFile Source="winston_tar.h.tpl" Destination="winston_tar.bin.h" Old="_SIZE_" New="$(TarSize)" />
  </Target>
  <!-- Clean -->
  <Target Name="CleanEmbed" AfterTargets="Clean">
    <Exec Command="del /F /Q *.bin.cpp 2&gt;nul" IgnoreExitCode="true" />
    <Exec Command="del /F /Q *.bin.h 2&gt;nul" IgnoreExitCode="true" />
    <Exec Command="del /F /Q $(NetFx) 2&gt;nul" IgnoreExitCode="true" />
  </Target>
  <!-- .NET download and embedding -->
  <Target Name="EmbedNetFx" BeforeTargets="PrepareForBuild">
    <DownloadFile Address="http://download.microsoft.com/download/1/4/A/14A6C422-0D3C-4811-A31F-5EF91A83C368/NDP46-KB3045560-Web.exe" FileName="$(NetFx)" />
    <Exec Command="tools\xd.exe -dnetfx_setup $(NetFx) &gt; netfx_setup.bin.cpp" />
    <GetFileSize FileName="$(NetFx)">
      <Output TaskParameter="FileSize" PropertyName="NetFxSize" />
    </GetFileSize>
    <ReplaceInFile Source="netfx_setup.h.tpl" Destination="netfx_setup.bin.h" Old="_SIZE_" New="$(NetFxSize)" />
  </Target>
  <!-- Elevate embedding -->
  <Target Name="EmbedElevate" BeforeTargets="PrepareForBuild">
    <Exec Command="tools\xd.exe -delevate_dll elevate.dll &gt; elevate_dll.bin.cpp" />
    <Exec Command="tools\xd.exe -delevate_exe elevate.exe &gt; elevate_exe.bin.cpp" />
    <GetFileSize FileName="elevate.dll">
      <Output TaskParameter="FileSize" PropertyName="ElevateDllSize" />
    </GetFileSize>
    <GetFileSize FileName="elevate.exe">
      <Output TaskParameter="FileSize" PropertyName="ElevateExeSize" />
    </GetFileSize>
    <ReplaceInFile Source="elevate.h.tpl" Destination="elevate.bin.h" Old="_DLL_SIZE_" New="$(ElevateDllSize)" />
    <ReplaceInFile Source="elevate.bin.h" Destination="elevate.bin.h" Old="_EXE_SIZE_" New="$(ElevateExeSize)" />
  </Target>
  <Target Name="Compact" AfterTargets="AfterBuild" Condition="'$(Configuration)' == 'Release' ">
